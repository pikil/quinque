{
  "name": "quinque",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && cp build/index.html build/404.html",
    "not-dev": "cp src/manifest.json static/manifest.json && vite dev",
    "not-build": "cp src/manifest.json static/manifest.json && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "playwright test",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.1",
    "@tailwindcss/postcss": "^4.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-svelte": "^2.36.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.5.0",
    "vite": "^6.0.3"
  },
  "type": "module",
  "dependencies": {
    "@discord/embedded-app-sdk": "^1.2.0",
    "firebase": "^10.8.0"
  }
}
